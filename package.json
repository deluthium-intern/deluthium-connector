{
  "name": "deluthium-connector",
  "version": "0.1.0",
  "private": true,
  "description": "Unified connector monorepo for Deluthium integrations across crypto MM infrastructure",
  "repository": {
    "type": "git",
    "url": "https://github.com/deluthium-intern/deluthium-connector.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@10.29.1",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write \"packages/*/src/**/*.ts\"",
    "format:check": "prettier --check \"packages/*/src/**/*.ts\""
  },
  "devDependencies": {
    "prettier": "^3.2.0",
    "turbo": "^2.8.0",
    "typescript": "^5.4.0"
  }
}

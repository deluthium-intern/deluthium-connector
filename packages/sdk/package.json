{
  "name": "@deluthium/sdk",
  "version": "0.1.0",
  "description": "Core SDK for Deluthium integrations -- API client, signer, chain config, types",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/deluthium-intern/deluthium-connector.git",
    "directory": "packages/sdk"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./signer": {
      "import": "./dist/signer/index.js",
      "types": "./dist/signer/index.d.ts"
    },
    "./chain": {
      "import": "./dist/chain/index.js",
      "types": "./dist/chain/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./errors": {
      "import": "./dist/errors/index.js",
      "types": "./dist/errors/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "node --import tsx --test 'tests/*.test.ts'",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist .turbo",
    "lint": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "ethers": "^6.13.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/ws": "^8.5.0",
    "tsx": "^4.19.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "deluthium",
    "rfq",
    "market-maker",
    "defi",
    "crypto",
    "sdk"
  ]
}

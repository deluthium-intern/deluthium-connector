{
  "name": "@deluthium/cli",
  "version": "0.1.0",
  "description": "Interactive CLI tool for setting up Deluthium adapter integrations",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/deluthium-intern/deluthium-connector.git",
    "directory": "packages/cli"
  },
  "type": "module",
  "bin": {
    "deluthium": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "node --import tsx --test 'tests/*.test.ts'",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist .turbo",
    "lint": "tsc -p tsconfig.json --noEmit",
    "start": "node --import tsx src/index.ts"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "tsx": "^4.19.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "deluthium",
    "cli",
    "rfq",
    "market-maker",
    "defi",
    "setup"
  ]
}
